   "source": [
    "function softmax(x::AbstractArray{T, N}; axis=N) where {T,N}\n",
    "    maxval = maximum(x; dims=axis)\n",
    "    exps = @. exp(x - maxval)\n",
    "    exps ./= sum(exps; dims=axis)\n",
    "end\n",
    "\n",
    "y = softmax(rand(2, 5))\n",
    "display(cumsum(y; dims=2));"
   ]
  }
